type User {
  id: ID! @id @unique
  username: String! @unique
  email: String! @unique
  firstName: String 
  lastName: String
  keeplist: [Book]
  uploadlist: [Book]
  reviewlist: [Review]
}

type Book {
  id: ID! @id @unique
  title: String!
  author: String!
  pubDate: String!
  priceStandard: Int
  coverSmallUrl: String
  publisher: String
  description: String
  stars: Int
}

type Review {
  id: ID! @id @unique
  score: Int
  context: String
}

