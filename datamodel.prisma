type User {
  id: ID! @unique
  username: String! @unique
  email: String! @unique
  firstName: String @default(value: "")
  lastName: String
  keeplist: String!
  uploadlist: [Book]
  reviewlist: [Review]
}

type Book {
  id: ID! @unique
  title: String!
  author: String!
  pubDate: String!
  priceStandard: Int
  coverSmallUrl: String
  publisher: String
  description: String
  stars: Int
}

type Review {
  id: ID! @unique
  score: Int
  context: String
}

