type User {
  id: ID! @id @unique
  username: String! @unique
  email: String! @unique
  firstName: String
  lastName: String
  uploadlist: [Book!]
  keeplist: [BOOK!]
}

type Book {
  id: ID! @id @unique
  title: String!
  author: String!
  pubDate: String!
  priceStandard: Int
  coverSmallUrl: String
  publisher: String
  description: String
  stars: Int
  uploadbylist: User! @relation(link: INLINE)
  keepedBy: [USER!] @relation(link: TABLE)
}

