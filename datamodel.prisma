type User {
  id: ID! @id @unique
  username: String! @unique
  email: String! @unique
  firstName: String
  lastName: String
  uploadlist: [Book!] @relation(name:"Upload")
  keeplist: [Book!] @relation(name:"Keep")
}

type Book {
  id: ID! @id @unique
  title: String!
  author: String!
  pubDate: DateTime!
  priceStandard: Int
  coverSmallUrl: String
  publisher: String
  description: String
  stars: Int
  uploadedby: User! @relation(link: INLINE, name:"Upload")
  keepedBy: [User!] @relation(link: TABLE, name:"Keep")

}

